<div>
    <h1>Welcome to profile page!</h1>
    <div>
        <table>
            <thead>
                <tr>
                    <th>id</th>
                    <th>fullName</th>
                    <th>username</th>
                    <th>password</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><%= user._id %></td>
                    <td><%= user.fullName %></td>
                    <td><%= user.username %></td>
                    <td><%= user.password.substr(0, 10) + "..." %></td>
                </tr>
            </tbody>
        </table>
    </div>
    <div>
        <a href="/logout" type="button">Logout</a>
        <a href="/">Home</a>
    </div>
</div>
